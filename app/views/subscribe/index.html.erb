<div class="span6 offset2">
  <h2>Subscribe</h2>
  <%= bootstrap_form_for(@feed, :remote => true, :url => "/api/add_feed", :method => :post, :html => { :id => "subscribe", :class => "well form-horizontal" }) do |f| %>
    <%= f.text_field :feed_url, :label => "Feed URL", :tabindex => 1 %>
    <%= f.control_group do %>
      <%= f.primary "Subscribe" %>
    <% end %>
  <% end %>
</div>
